{
  "imports": {
    "hono": "jsr:@hono/hono@^4.10.7",
    "mongodb": "npm:mongodb@^6.10.0",
    "better-auth": "npm:better-auth@^1.3.10",
    "better-auth/adapters/mongodb": "npm:better-auth@^1.3.10/adapters/mongodb"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --env-file=.env --watch --allow-sys main.ts",
    "start": "deno run --allow-net --allow-env --env-file=.env --allow-sys main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
